
package hello

import "io"

enum Result {
  Ok,
  Err(int, string),

  func ok_then() Result {
    Ok
  }

  func failed() Result {
    Err(2, "connection timeout")
  }

  func test() {
    io.puts("Result.test()");
  }
}

func get_result() Result {
  io.puts("get_result()")
  Result.Err(10, "bad file descriptor")
}
