func test_while(a int, b int) int {
  res := 0
  while a > b {
    res -= 1
    a -= 1
  }
  res
}

func test_while2(arr [int]) int {
  res := 0
  i := 0
  size := arr.length()
  while i < size {
    res += arr[i]
    i += 1
  }
  return res
}

