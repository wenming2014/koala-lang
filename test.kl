enum Option {
  None,
  Some(int, string),

  func is_some() bool {
    return (self == None) ? false : true
  }

  func is_none() bool {
    return (self == None) ? true : false
  }
}

opt := Option.None
opt
opt.is_some()
opt.is_none()
Option.None.is_some()
Option.Some(1, "foo").is_none()

opt = Option.Some(1, "foo")
opt.is_some()
opt.is_none()

opt = Option.None
opt.is_some()
opt.is_none()

opt == Option.Some(1, "foo")
assert.equal(Option.Some(1, "foo").is_some(), true)
