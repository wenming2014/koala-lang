
import "io"
import "assert"

var i int
var s string

(i, s) = (100, "foo")
assert.equal(i, 100)
assert.equal(s, "foo")

(_, s) = (100, "bar")
assert.equal(s, "bar")

i = (200, 300).0
assert.equal(i, 200)

tuple := (200, 300)
i = tuple.1
assert.equal(i, 300)

(j, k) := (1, 2)
assert.equal(j, 1)
assert.equal(k, 2)

var (l, m) = (1, "jack")
var (n, o) (int, string) = (2, "rose")

func test_tuple(t (int, string)) string {
  t.1
}

func test_tuple2(v1 int, v2 string)) (int, string) {
  (v1, v2)
}

s = test_tuple((1, "jack"))
assert.equal(s, "jack")

(l, m) = test_tuple2(2, "rose")
assert.equal(l, 2)
assert.equal(m, "rose")
