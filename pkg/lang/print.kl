import "github.com/fmt"
import "sys"

func printf(format string, args ...) {
  fmt.fprintf(sys.stdout, format, args)
}

func println(args ...) {
  fmt.fprintln(sys.stdout, args)
}

func print(args ...) {
  fmt.fprint(sys.stdout, args)
}
